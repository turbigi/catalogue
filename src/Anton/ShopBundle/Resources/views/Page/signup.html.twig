{% extends 'AntonShopBundle::layout.html.twig' %}

{% block title %}Sign up{% endblock%}

{% block body %}
    <header>
        <h1>Sign up</h1>
    </header>

    {{ form_start(form, { 'attr': {'class': 'anton'} }) }}
    {{ form_errors(form) }}
    {{ form_widget(form) }}
    <input type="submit" value="Submit" />
    {{ form_end(form) }}
{% endblock %}